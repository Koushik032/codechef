#include <iostream>
using namespace std;

int main() {
    int t;
    cin >> t;
    int a,b,c;
    while(t--){
        cin >> a >> b >> c;
        int d=a+b;
        int e=b+c;
        int f=a+c;
        if(d==c){
            cout << "YES" << endl;
        }
        else if(e==a){
            cout << "YES" << endl;
        }
        else if(f==b){
            cout << "YES" << endl;
        }
        else{
            cout << "NO" << endl;
        }
        
    }
	// your code goes here
	return 0;
}
