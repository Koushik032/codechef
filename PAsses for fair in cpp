#include <iostream>
using namespace std;

int main() {
    int t;
    cin >> t;
    int x,y;
    for(int i=0;i<t;i++)
    {
        cin >> x >> y;
        if(x<y)
        {
            cout << "YES" <<endl;
        }
        else
        {
            cout << "NO" <<endl;
        }
    }
	// your code goes here
	return 0;
}
