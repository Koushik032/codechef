#include<bits/stdc++.h>
using namespace std;
#define ll long long
#define max1 10000
#define pb push_back
#define fi first
#define se second
#define vst() sort(v.begin(),v.end())
#define vec() vector<char>v
const int mx=2e5+123;
ll sum[mx],a[mx];
int main(){
   ll a,b,c,d;
   cin>>a>>b>>c>>d;

   string s;
   cin>>s;
   ll sum=0;
   for(ll i=0;i<s.size();i++){
      if(s[i]=='1'){
         sum+=a;
      }
      else if(s[i]=='2'){
         sum+=b;
      }
      else if(s[i]=='3'){
         sum+=c;
      }
      else sum+=d;
   }
   cout<<sum<<endl;
    return 0;
}
