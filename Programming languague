#include<bits/stdc++.h>
using namespace std;
#define Royscode() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define endl '\n';
#define max 10000
#define ll long long
int main()
{
    Royscode();
    int t;
    cin >> t;
    while(t--)
    {
        int a,b,a1,b1,a2,b2;
        cin >> a >> b >> a1 >> b1 >> a2 >> b2;

        if((a==a1 || b==a1) && (a==b1 || b==b1))
        {
            cout << 1 << endl;
        }
        else if((a==a2 || b==a2) && (a==b2 || b==b2))
        {
            cout << 2 << endl;
        }
        else
        {
            cout << 0 << endl;
        }
    
    }
    return 0;
}
