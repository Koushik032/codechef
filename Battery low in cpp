#include <iostream>
using namespace std;

int main() {
    int t,n;
    cin >> t;
    for(int i=1;i<=t;i++)
    {
        cin >> n;
        if(n<=15)
        {
            cout << "YES" << endl;
        }
        else
        {
            cout << "NO" << endl;
        }
    }
	// your code goes here
	return 0;
}
