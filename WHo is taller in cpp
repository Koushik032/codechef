#include <iostream>
using namespace std;

int main() {
    int t,i;
    cin >> t;
    
    int a,b;
    for(i=1;i<=t;i++)
    {
        cin >> a >> b;
        
        if(a>b)
        {
            cout << "A" <<endl;
        }
        else
        {
            cout << "B" <<endl;
        }
    }
	// your code goes here
	return 0;
}
