#include <iostream>
using namespace std;

int main() {
    int t,x;
    cin >> t;
    for(int i=1;i<=t;i++)
    {
        cin >> x;
        if(x%4==0)
        {
            cout << "Good" <<endl;
        }
        else 
        {
            cout << "Not Good" <<endl;
        }
    }
	// your code goes here
	return 0;
}
