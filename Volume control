#include <stdio.h>

int main(void) {
    int n,i,max,min;
    scanf("%d",&n);

    int a,b,count=0;

    for(i=0;i<n;i++)
    {
        scanf("%d%d",&a,&b);

        if(a>b)
        {
            max=a;
            min=b;
        }
        else
        {
            max=b;
            min=a;
        }

        int x=max-min;

        printf("%d\n",x);
    }

	// your code goes here
	return 0;
}

