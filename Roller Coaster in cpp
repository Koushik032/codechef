#include <iostream>
using namespace std;

int main() {
    int t;
    cin >> t;
    int a,b;
    for(int i=0;i<t;i++)
    {
        cin >> a >> b;
        
        if(a>=b)
        {
            cout << "YES" <<endl;
        }
        else
        {
            cout << "NO" << endl;
        }
    }
	// your code goes here
	return 0;
}
