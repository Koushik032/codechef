#include <iostream>
using namespace std;

int main() {
    int t,a,b;
    cin >> t;
    for(int i=1;i<=t;i++)
    {
        cin >> a >> b;;
        
        int x=a+b;

        if(x%2==0)
        {
            cout << "YES" <<endl;
        }
        else
        {
            cout << "NO" <<endl;
        }
    }
	// your code goes here
	return 0;
}
