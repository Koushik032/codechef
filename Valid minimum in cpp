#include <iostream>
using namespace std;

int main() {
    int t;
    cin >> t;
    int a,b,c;
    int x,y,z;
    for(int i=0;i<t;i++)
    {
        cin >> a >> b >> c;
        if(a<b)
        {
            x=a;
        }
        else
        {
            x=b;
        }
        if(b<c)
        {
            y=b;
        }
        else
        {
            y=c;
        }
        if(a<c)
        {
            z=a;
        }
        else
        {
            z=c;
        }
        if(x==y && y==z)
        {
            cout << "YES" << endl;
        }
        else
        {
            cout << "NO" << endl;
        }
    }
	// your code goes here
	return 0;
}
