#include <stdio.h>

int main(void) {
    int t,i;
    int a[1000];

    scanf("%d",&t);
    for(int i=0;i<t;i++)
    {
        scanf("%d",&a[i]);
    }
    int result;
    for(i=0;i<t;i++)
    {
        result=(a[i]+a[i])*5;
        printf("%d\n",result);
    }


	// your code goes here
	return 0;
}

